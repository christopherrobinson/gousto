---
type Props = Record<string, never>;
---

<div class="animate-pulse  bg-white  border  border-secondary-100  flex  flex-col  group  h-full  overflow-hidden  rounded-lg  transition">
  <div class="aspect-[1/1]  bg-secondary-50  w-full"></div>
  <div class="flex flex-col justify-between h-full p-4">
    <div class="py-1  mb-5  space-y-2">
      <div class="bg-secondary-200  h-4  rounded  w-7/8"></div>
      <div class="bg-secondary-200  h-4  rounded  w-2/3"></div>
    </div>
    <div class="flex  items-center  justify-between  overflow-hidden  space-x-8  text-sm">
      <div class="flex  items-center  space-x-2">
        <div class="bg-secondary-200  h-4  rounded-full  w-4"></div>
        <div class="bg-secondary-200  h-4  rounded  w-12"></div>
      </div>
      <div class="flex  items-center  space-x-2">
        <div class="bg-secondary-200  h-4  rounded-full  w-4"></div>
        <div class="bg-secondary-200  h-4  rounded  w-16"></div>
      </div>
    </div>
  </div>
</div>
